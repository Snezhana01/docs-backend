{
  "name": "DOCS",
  "version": "3.3.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:dev": "docker compose --env-file deploy/envs/.env.development -p docs-backend -f deploy/docker-compose.local.yml up -d",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development ts-node src/main.ts",
    "start:prod": "node dist/src/main.js",
    "watch:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "typeorm": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "typeorm-seeding": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/@jorgebodega/typeorm-seeding/dist/cli.js",
    "migration:run": "yarn run typeorm migration:run -d src/configs/ormconfig.ts",
    "migration:generate": "yarn run typeorm migration:generate src/database/migrations/migration -d src/configs/ormconfig.ts",
    "migration:create": "yarn run typeorm migration:create src/database/migrations/migration",
    "migration:down": "yarn run typeorm migration:revert -d src/configs/ormconfig.ts",
    "schema:drop": "yarn run typeorm schema:drop -d src/configs/ormconfig.ts",
    "seed:run": "yarn run typeorm-seeding -d src/configs/seeds.ormconfig.ts seed src/database/seeds/*.ts",
    "db:setup": "yarn run migration:run && yarn run seed:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:dev": "cross-env NODE_ENV=development jest --config jest.config.ts --runInBand",
    "test": "cross-env NODE_ENV=test jest --config jest.config.ts --runInBand",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=development jest --config ./test/jest-e2e.config.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@a2seven/yoo-checkout": "^1.1.4",
    "@aws-sdk/client-s3": "3.410.0",
    "@elastic/elasticsearch": "7.9.0",
    "@jorgebodega/typeorm-seeding": "6.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.0.1",
    "@nestjs/core": "10.0.0",
    "@nestjs/elasticsearch": "10.0.1",
    "@nestjs/event-emitter": "2.0.2",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/passport": "10.0.1",
    "@nestjs/platform-express": "10.2.4",
    "@nestjs/platform-socket.io": "10.2.4",
    "@nestjs/schedule": "3.0.3",
    "@nestjs/swagger": "7.1.10",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "@nestjs/websockets": "10.2.4",
    "@sentry/node": "^8.7.0",
    "async": "3.2.4",
    "async-mutex": "0.4.0",
    "aws-sdk": "2.1454.0",
    "axios": "1.5.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cross-env": "7.0.3",
    "form-data": "4.0.0",
    "heic-convert": "1.2.4",
    "helmet": "^7.1.0",
    "ip-range-check": "^0.2.0",
    "morgan": "1.10.0",
    "multer-s3": "3.0.1",
    "passport": "0.6.0",
    "passport-jwt": "^4.0.1",
    "path": "0.12.7",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sharp": "0.32.6",
    "ts-jest": "^29.1.4",
    "tsconfig-paths": "3.10.1",
    "typeorm": "0.3.17",
    "typesafe-event-emitter": "1.1.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@darraghor/eslint-plugin-nestjs-typed": "4.0.16",
    "@nestjs/cli": "10.1.17",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@ngrok/ngrok": "^1.3.0",
